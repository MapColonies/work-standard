{
  "name": "standard",
  "version": "1.0.0",
  "description": "This repo describes the code and work standards for vector team",
  "main": "index.js",
  "scripts": {
    "test:unit": "nyc --check-coverage --lines 95 --reporter=html --reporter=text RUN_UNIT_TEST_COMMAND",
    "test:integration": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**",
    "release": "standard-version",
    "test": "npm run lint && npm run test:unit && npm run test:integration"
  },
  "directories": {
    "test": "tests"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": "src"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitlint": "^9.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "nyc": "^15.1.0",
    "standard-version": "^8.0.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
